<html>
<head>
    <title>{{ site.title }}</title>
    <meta charset="utf-8" />

    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />

    <link rel="stylesheet" href="{{ 'assets/css/main.css' | relative_url }} " />
    <link rel="stylesheet" href="{{ 'assets/fontawesome/css/fontawesome.css' | relative_url }} " />
    <link rel="stylesheet" href="{{ 'assets/fontawesome/css/solid.css' | relative_url }} " />
    <link rel="stylesheet" href="{{ 'assets/fontawesome/css/brands.css' | relative_url }} " />

    {% for href in site.customStyles %}
        <link rel="stylesheet" href="{{ href | relative_url }} " />
    {% endfor %}

    {% if jekyll.environment == "development" %}
        <link rel="stylesheet" href="{{ 'assets/css/scratch.css' | relative_url }}" />
    {% endif %}
</head>
<body class="{{ page.cssClass }} {{ site.bodyClasses | join: " " }} is-preload">
    <div id="page-wrapper">